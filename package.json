{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A SPA boilerplate with React.",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "build:dll": "webpack --config build/webpack.dll.conf.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --ext .js,jsx . --fix --cache",
    "lint:css": "stylelint 'src/**/*.scss' --syntax=scss --fix --cache",
    "test": "NODE_ENV=testing jest"
  },
  "repository": {
    "type": "git",
    "url": "git@git.internal.yunify.com:elvis/react-boilerplate.git"
  },
  "keywords": [
    "boilerplate"
  ],
  "license": "UNLICENSED",
  "private": true,
  "author": "QingCloud F2E Group",
  "bundledDependencies": [],
  "dependencies": {
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
